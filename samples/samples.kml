&lt;span style=&quot;color: #408080; font-style: italic&quot;&gt;# A simple Polygon with a hole in it.&lt;/span&gt;
pol &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; kml&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;newpolygon(name&lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt;&lt;span style=&quot;color: #BA2121&quot;&gt;&amp;quot;Atrium Garden&amp;quot;&lt;/span&gt;,
                     outerboundaryis&lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt;[(&lt;span style=&quot;color: #666666&quot;&gt;18.43348&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98985&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43387&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.99004262216968&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43410&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98972&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43371&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98952&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43348&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98985&lt;/span&gt;)],
                     innerboundaryis&lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt;[[(&lt;span style=&quot;color: #666666&quot;&gt;18.43360&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98982&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43386&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98995&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43401&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98974&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43376&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98962&lt;/span&gt;), (&lt;span style=&quot;color: #666666&quot;&gt;18.43360&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.98982&lt;/span&gt;)]])

&lt;span style=&quot;color: #408080; font-style: italic&quot;&gt;# Saving&lt;/span&gt;
kml&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;save(os&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;path&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;splitext(__file__)[&lt;span style=&quot;color: #666666&quot;&gt;0&lt;/span&gt;] &lt;span style=&quot;color: #666666&quot;&gt;+&lt;/span&gt; &lt;span style=&quot;color: #BA2121&quot;&gt;&amp;quot;.kml&amp;quot;&lt;/span&gt;)
&lt;span style=&quot;color: #408080; font-style: italic&quot;&gt;#kml.savekmz(os.path.splitext(__file__)[0] + &amp;quot;.kmz&amp;quot;) # uncomment to save to kmz&lt;/span&gt;
&lt;span style=&quot;color: #408080; font-style: italic&quot;&gt;#print kml.kml() # uncomment to see the kml printed to screen&lt;/span&gt;
