&lt;span style=&quot;color: #408080; font-style: italic&quot;&gt;# A simple Point&lt;/span&gt;
kml&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;newpoint(name&lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt;&lt;span style=&quot;color: #BA2121&quot;&gt;&amp;quot;Kirstenbosch&amp;quot;&lt;/span&gt;, coords&lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt;[(&lt;span style=&quot;color: #666666&quot;&gt;18.432314&lt;/span&gt;,&lt;span style=&quot;color: #666666&quot;&gt;-33.988862&lt;/span&gt;)])

