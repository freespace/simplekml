    pnt &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; kml&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;newpoint()
    pnt&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;name &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; city
