    pnt&lt;span style=&quot;color: #666666&quot;&gt;.&lt;/span&gt;coords &lt;span style=&quot;color: #666666&quot;&gt;=&lt;/span&gt; [(lon, lat)]

&lt;span style=&quot;color: #408080; font-style: italic&quot;&gt;# Save the KML&lt;/span&gt;
